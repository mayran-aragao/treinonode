{{> header}}
<h1>Mude sua senha</h1>

<form method="POST" >
    
    <label>
        Nova Senha:
        <input type="password" name="password" />
    </label>
    <br/>

    <label>
        Confirmar nova senha:
        <input type="password" name="password-confirm" />
    </label>
    <br/>
    
    <br/>
    <label>
        <input type="submit" value="Salvar"  />
        
    </label>
</form>